{
   "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
      "react/prefer-stateless-function": [0, { "ignorePureComponents": false }]
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "settings": {
      "import/core-modules": [
        "app",
        "components",
        "resources",
        "services",
        "screens",
        "stores",
        "lib"
      ]
    },
    "env": {
        "browser": true
    }
}
